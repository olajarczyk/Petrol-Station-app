        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.936"
        app:srcCompat="@drawable/app_123" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="289dp"
        android:layout_height="188dp"
